{
  "defaultSeverity": "warning",
  "extends": ["tslint:recommended", "tslint-react", "plugin:json/recommended"],
  "jsRules": {},
  "rules": {
    "trailing-comma": [false],
    "no-var-requires": false,
    "quotemark": [true, "single", "jsx-double"],
    "jsx-self-close": true,
    "array-type": false,
    "jsx-boolean-value": false
  },
  "rulesDirectory": []
}
